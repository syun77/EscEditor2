def knob
{
	ITEM_CHK(ITEM_SILVER_KEY)
	if($RET) {
		// 銀色の扉を開いた
		ITEM_DEL(ITEM_SILVER_KEY)
		%UNLOCK_SILVER_DOOR = true
		":カギを使って扉を開いた"
	}
	else {
		":銀色のドアノブがついている。扉は開かない"
	}
}

